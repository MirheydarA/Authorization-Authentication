@using Fiorello.Areas.Admin.ViewModels.User
@model UserDetailsVM

<h3>UserName</h3>
<span>@Model.UserName</span>

<h3>Fullname</h3>
<span>@Model.FullName</span>

<h3>Phone Number</h3>
<span>@Model.PhoneNumber</span>

<h3>Country</h3>
<span>@Model.Country</span>

<h3>Email</h3>
<span>@Model.Email</span>

<h3>Roles</h3>
@foreach (var role in Model.Roles)
{
<span>@role</span>
}

